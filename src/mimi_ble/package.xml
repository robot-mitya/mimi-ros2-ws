<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>mimi_ble</name>
  <version>0.0.0</version>
  <description>This package contains the mimi_ble_node, a Bluetooth gateway node responsible for wireless communication between the Mimi robot and external applications.</description>
  <maintainer email="DzakhovDS@gmail.com">DmitryDzz</maintainer>
  <license>MIT</license>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>ament_xmllint</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
